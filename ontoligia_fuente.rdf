        <rdfs:domain rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#CompeticionNacional"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- numeroEquipos -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#numeroEquipos">
