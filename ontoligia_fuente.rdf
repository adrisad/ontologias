    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#nombreCompeticion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
