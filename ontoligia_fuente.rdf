    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Estadio">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
    </owl:Class>
    
    <!-- Competicion -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Competicion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
    </owl:Class>
    
    <!-- CompeticionNacional -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#CompeticionNacional">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Competicion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#perteneceAPais"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#tieneEquipoParticipante"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Club"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- Liga -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Liga">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#CompeticionNacional"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Copa"/>
    </owl:Class>
    
    <!-- Copa -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Copa">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#CompeticionNacional"/>
    </owl:Class>
    
    <!-- deportista (ya existía) -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#deportista">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
    </owl:Class>
    
    <!-- futbol (ya existía) -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
    
    <!-- reglas (ya existía) -->
    <owl:Class rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#reglas">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
    </owl:Class>
</rdf:RDF>