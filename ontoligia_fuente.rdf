        <rdfs:range rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Estadio"/>
    </owl:ObjectProperty>
    
    <!-- perteneceAPais -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#perteneceAPais">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#CompeticionNacional"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#futbol"/>
    </owl:ObjectProperty>
    
