        <rdfs:domain rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#CompeticionNacional"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- numeroJornadas -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#numeroJornadas">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Liga"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    
    <!-- sistemaCompeticion -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#sistemaCompeticion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Liga"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- puntosVictoria -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#puntosVictoria">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/adrian/ontologies/2025/8/untitled-ontology-12#Liga"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
